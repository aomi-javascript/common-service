{
  "name": "@aomi/common-service",
  "version": "1.0.6",
  "repository": "git@github.com:aomi-web/common-service.git",
  "author": "sean <sean.snow@live.com>",
  "license": "MIT",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "miniprogram": ".",
  "scripts": {
    "clean": "find . -path \"./node_modules\" -prune -o -name \"*.js\" -o -name \"*.js.map\" -o -name \"*.d.ts\" |xargs rm -f | echo ''",
    "build": "rm -rf esm cjs && tsc -d && tsc --project tsconfig.cjs.json",
    "watch": "tsc -d && tsc -w",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@aomi/utils": "^1.0.10",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "object-path": "^0.11.8"
  }
}
